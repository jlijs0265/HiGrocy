<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zerock.mapper.orderMapper">
	  
	<select id="getlist" resultType="org.zerock.domain.orderVO">
  			select sysdate from dual;
  			
  	</select>
	
	<insert id="insertOrder" parameterType="org.zerock.domain.orderVO">
			insert into item (item_code, type)
			VALUES(seq_item.nextval, #{item_code}, #{type})
	</insert>
	
</mapper>